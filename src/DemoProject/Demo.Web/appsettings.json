{
  "ConnectionStrings": {
    "DemoConnection": "Server=myServerAddress;Database=myDataBase;User Id=myUsername;Password=myPassword;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/web-log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Smtp": {
    "Host": "",
    "Username": "yourEmail",
    "Password": "setpassword",
    "Port": "465"
  },
  "AllowedHosts": "*"
}
